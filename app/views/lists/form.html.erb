<div class="container">
  <%= form_with(model: @list, local: true) do |f| %>
    <div class="field">
      <%= f.label :name %> </br>
      <%= f.text_field :name, class: "form-control" %>
    </div>

    <div class="actions">
      <%= f.submit "Save List", class: :button %>
    </div>
  <% end %>
</div>
